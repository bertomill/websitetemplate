{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.py",
      "use": "@vercel/python"
    },
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/search-templates",
      "dest": "api/search-templates/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
} 